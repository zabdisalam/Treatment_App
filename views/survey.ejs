<style>
.submit_btn button:hover {
  background-color: #555;
  color: white;
}
.submit_btn button{
  float: right; 
  text-decoration: none; 
  background-color: #3498DB; 
  color: black;
  padding: 12px;
  border-radius: 12px;
  
}

</style>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<a href="/users/logout" class="btn btn-secondary" style="float: right; padding-bottom: 4px;">Logout</a>
<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <strong style="font-weight: lighter; " id="name"><%= user.name %></strong></p>
<div style="background-color: rgb(255, 255, 255);">
      <h4>Answer the following questions</h4>
      <form id="quiz" name="quiz">
        <h3 id="q1">Favorite Subject:</h3><br>
        <label class="container">
          <input type="radio" class="mc1" id="mc" name="question1" value="Science">Science<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc2" id="mc" name="question1" value="Math">Math<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc3" id="mc" name="question1" value="Comp sci">Computer Science<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc4" id="mc" name="question1" value="Social Science">Social Science<br>
          <span class="checkmark"></span>
        </label>
          <h3 id="q2">Hobbies:</h3><br>
        <label class="container">
          <input type="radio" class="mc5" id="mc" name="question2" value="Sports">Sports<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc6" id="mc" name="question2" value="Music">Music<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc7" id="mc" name="question2" value="Literature">Literature<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc8" id="mc" name="question2" value="Technology">Technology<br>
          <span class="checkmark"></span>
        </label>
         <h3 id="q3">Intensity:</h3><br>
        <label class="container">
          <input type="radio" class="mc9" id="mc" name="question3" value="Intense">Intense<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc10" id="mc" name="question3" value="Highly Intense">Highly Intense<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc11" id="mc" name="question3" value="Laissez-faire">Laissez-faire<br>
          <span class="checkmark"></span>
        </label>
        <h4 id="q4">probability of getting hired after 4 years:</h4><br>
        <label class="container">
          <input type="radio" class="mc12" id="mc" name="question4" value="Low">Low<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc13" id="mc" name="question4" value="Medium">Medium<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc14" id="mc" name="question4" value="High">High<br>
          <span class="checkmark"></span>
        </label>
        <h5 id="q5">preferred work environment:</h5><br>
        <label class="container">
          <input type="radio" class="mc15" id="mc" name="question5" value="Office">Office<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc16" id="mc" name="question5" value="Standing">Standing<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc17" id="mc" name="question5" value="Traveling">Traveling<br>
          <span class="checkmark"></span>
        </label>
        <label class="container">
          <input type="radio" class="mc18" id="mc" name="question5" value="Outdoors">Outdoors<br>
          <span class="checkmark"></span>
        </label>
     </form>
     <button name="submit" onclick="check()">Submit</button>

<div id="output" name="output"></div>
<p id="message"></p>
<p id="number_correct"></p>







